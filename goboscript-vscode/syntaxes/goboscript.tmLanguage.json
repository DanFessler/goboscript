{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "goboscript",
	"patterns": [
		{"include": "#commentmultiline"},
		{"include": "#commentsingleline"},
		{"include": "#string"},
		{"include": "#all"}
	],
	"repository": {
		"all": {
			"patterns": [
				{
					"name": "keyword.control",
					"match": "\\b(use|costumes|sounds|def|nowrap|if|else|elseif|repeat|until|onflag|onclone|onevent|onkey|onclick)\\b"
				},{
					"name": "keyword.operator",
					"match": "[+\\-*/!&%|=<>]"
				},{
					"name": "punctuation",
					"match": "[;,.]"
				},{
					"name": "constant.numeric",
					"match": "\\b[0-9.]+\\b"
				},{
					"name": "variable.parameter",
					"match": "@[a-zA-Z_0-9]+"
				},{
					"name": "variable.parameter",
					"match": "$[a-zA-Z_0-9]+"
				},{
					"name": "entity.name",
					"match": "\\b[a-zA-Z_0-9]+\\s*\\("
				},{
					"name": "entity.name",
					"match": "\\b(if|until|repeat|forever|say|sayfor|think|thinkfor|switchcostume|nextcostume|switchbackdrop|nextbackdrop|changesize|setsize|cleargraphiceffects|setgraphiceffect|changegraphiceffect|hide|show|gotofrontback|goforwardbackwardlayers|eraseall|stamp|pendown|penup|setpencolor|changepencolorparam|setpencolorparam|changepensize|setpensize|ask|setdraggable|setnotdraggable|resettimer|move|turnright|turnleft|gotosprite|goto|glidetosprite|glide|point|pointtowards|changex|changey|setx|sety|ifonedgebounce|setrotationstyle|broadcast|broadcastandwait|wait|waituntil|createclone|deletethisclone|stop|setvariable|changevariable|showvariable|hidevariable|addtolist|deleteoflist|deletealloflist|insertatlist|replaceitemoflist|showlist|hidelist|playsounduntildone|startsound|stopallsounds|setsoundeffect|changesoundeffect|clearsoundeffects|changevolume|setvolume|setcoloreffect|setfisheyeeffect|setwhirleffect|setpixelateeffect|setmosaiceffect|setbrightnesseffect|setghosteffect|changecoloreffect|changefisheyeeffect|changewhirleffect|changepixelateeffect|changemosaiceffect|changebrightnesseffect|changeghosteffect|gotofront|gotoback|goforward|gobackward|changepenhue|changepensaturation|changepenbrightness|changepentransparency|setpenhue|setpensaturation|setpenbrightness|setpentransparency|gotomousepointer|gotorandomposition|glidetomousepointer|glidetorandomposition|pointtowardsmousepointer|setrotationstyleleftright|setrotationstyleallaround|setrotationstyledontrotate|sa_breakpoint|sa_log|sa_warn|sa_error|setpitcheffect|changepitcheffect|setpaneffect|changepaneffect|return|exit|stopotherscripts)\\b"
				},{
					"name": "entity.name",
					"match": "\\b(insert|length|add|delete|contains|index)\\b"
				},
				{
					"name": "constant",
					"match": "\\b(true|false)\\b"
				}
			]
		},
		"string": {
			"name": "string.quoted.double",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
					"name": "constant.character.escape",
					"match": "\\\\."
			}]
		},
		"commentmultiline": {
			"name": "comment.block",
			"begin": "/\\*",
			"end": "\\*/",
			"patterns": []
		},
		"commentsingleline": {
			"name": "comment.line",
			"begin": "//",
			"end": "$",
			"patterns": []
		}
	},
	"scopeName": "source.goboscript"
}
